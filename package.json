{
  "name": "generator-clowder-generator",
  "version": "0.1.0",
  "description": "A generator to create new generators in typescript and test support",
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "author": "leddzip",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf generators",
    "test": "jest --config jestconfig.json",
    "preit": "npm run build",
    "it": "cucumber-js",
    "postit": "npm run clean",
    "prebuild": "npm run clean",
    "build": "tsc && cp -r src/app/templates generators/app/templates"
  },
  "dependencies": {
    "yeoman-generator": "^5.7.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.9.1",
    "@types/jest": "^29.2.4",
    "@types/yeoman-generator": "^5.2.11",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
